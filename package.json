{
  "name": "example-electron-app-react",
  "version": "1.0.0",
  "description": "Electron react example",
  "main": "dist/electron/main.js",
  "author": "marcos97britos@gmail.com",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "concurrently -k \"npm run dev:frontend\" \"npm run dev:electron\"",
    "dev:frontend": "cd frontend && npm run dev",
    "dev:electron": "wait-on tcp:5173 && cross-env NODE_ENV=development electron .",
    "build": "npm run build:frontend && npm run build:electron",
    "build:frontend": "cd frontend && npm run build",
    "build:electron": "tsc && electron-builder",
    "start": "electron ."
  },
  "keywords": [],
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^9.1.2",
    "cross-env": "^7.0.3",
    "electron": "^35.1.4",
    "electron-builder": "^26.0.12",
    "wait-on": "^8.0.3"
  }
}
